{
  "name": "spotify-react",
  "version": "1.0.0",
  "description": "Test application of API features for Spotify service using React and Redux",
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "webpack-dev-server --content-base ./src/",
    "lint": "node_modules/.bin/esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Salivonenko Oleksandr",
  "license": "MIT",
  "dependencies": {
